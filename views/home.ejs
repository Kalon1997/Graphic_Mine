  <body>
    <!-- <div class="theme"> -->
     <!-- style = "background-color: #e3f2fd;" -->
    <nav class="navbar navv navbar-expand-sm  ">
      <a href="/" class="navbar-brand graphicmine">Graphic_Mine</a>
       <!-- <img src="\images\pplogo.png" class="pplogo" alt="Site logo"> -->
      <button class="navbar-toggler" data-toggle="collapse" data-target="#menu">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="menu">
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a href="/feed" class="nav-link nav-link2">Feed</a>
          </li>

          <li>
            <a href="/contact" class="nav-link nav-link2">Contact</a>
          </li>
           <% if (!isAuthed) { %>

             <li>
               <a href="/login" class="nav-link nav-link2 ">Login</a>
             </li>

            <% } else { %>

              <li>
                  <a href="/addProd" class="nav-link nav-link2 ">Add products</a>
              </li>
              <li>
                <a href="/user/<%= currUserId %>" class="nav-link nav-link2 ">Me</a>
              </li>

              <li>
                <form class="logout" action="/logout" method="post">
                <button type="submit" method="POST" class="btn btn-primary btn-sm nav-link2">Logout</button>
                </form>
              </li>
            <% } %>

        </ul>
      <!-- </div> -->

    </nav>

<div class="sec_2">


  <div> 
    <span class="text1">Share your graphic creativities with the world!</span>

  </div>

  <% if (!isAuthed) { %>
  <div>
    <form class="btn-padding" action="/signup" method="GET">
      <!-- <button type="submit" class=" btn-padding adding-radius padding-titles btn-primary btn-lg btn btn-round-lg joinow"><h3>Join Now</h3></button> -->
      <button type="submit" class="btn btn-outline-success adding-radius padding-titles btn-padding btn-lg btn-round-lg joinow"><h3>Join Now</h3></button>
  </form>
  </div>
  <% } %>
